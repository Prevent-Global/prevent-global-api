{
	"info": {
		"_postman_id": "31a6b54b-9eb0-4922-8397-d6162fa70dfb",
		"name": "Prevent.Global Mobile API",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Register user information",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "20c80095-f908-4773-9312-007a6a6642c1",
						"exec": [
							"",
							"pm.test(\"Creates User With Id\", function () {",
							"    var jsonData = pm.response.json(); ",
							"    ",
							"    pm.response.to.have.status(200);",
							"    pm.expect(jsonData.id.length > 0).to.be.true;",
							"    ",
							"    pm.variables.set(\"userId\", jsonData.id);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\"recentTest\":\"positive\"}"
				},
				"url": {
					"raw": "{{serverUrl}}api/user/register",
					"host": [
						"{{serverUrl}}api"
					],
					"path": [
						"user",
						"register"
					]
				}
			},
			"response": []
		},
		{
			"name": "Upload KML File",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "fc293dc6-b415-478c-8f3a-e10c512b721f",
						"exec": [
							"",
							"pm.test(\"Creates User With Id\", function () {",
							"    pm.response.to.have.status(201);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "multipart/form-data",
						"type": "text"
					}
				],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "locationHistory",
							"type": "file",
							"src": "testData/history-2016-10-17.kml"
						},
						{
							"key": "userId",
							"value": "{{userId}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{serverUrl}}api/kml/upload",
					"host": [
						"{{serverUrl}}api"
					],
					"path": [
						"kml",
						"upload"
					]
				}
			},
			"response": []
		}
	],
	"protocolProfileBehavior": {}
}